<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#000000" />

            <solid android:color="#ffffff"/>
            <size
                android:width="30dp"
                android:height="30dp" />
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
            <corners android:radius="4dp"/>
        </shape>
    </item>

</layer-list>